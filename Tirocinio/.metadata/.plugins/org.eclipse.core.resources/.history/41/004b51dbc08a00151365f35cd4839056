package it.sp4te.css.main;

import it.sp4te.css.agents.PrimaryUser;
import it.sp4te.css.model.Signal;
import it.sp4te.css.signalprocessing.SignalProcessor;
import it.sp4te.css.signalprocessing.Utils;

public class ThresholdCreator {
	public static void main(String args[]) throws Exception {
		PrimaryUser PU= new PrimaryUser();
		//creo il segnale
		Signal s = PU.createAndSend(1000);
		
	Utils.createThreshold(-30, 5, 1000, SignalProcessor.computeEnergy(s), 1000);
	}
}
